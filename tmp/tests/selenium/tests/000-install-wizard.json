{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://127.0.0.1:8080/install.php"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-profile--4"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-mysql-database"
      },
      "text": "drupal"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-mysql-username"
      },
      "text": "root"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-save"
      }
    },
    {
      "type": "waitForTitle",
      "title": "Configure site | Drupal"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-site-name"
      },
      "text": "Skeletor"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-site-mail"
      },
      "text": "skeletor@example.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-account-name"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-account-pass-pass1"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "edit-account-pass-pass2"
      },
      "text": "admin"
    },
    {
      "type": "setElementNotSelected",
      "locator": {
        "type": "id",
        "value": "edit-update-status-module-1"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "edit-submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Visit your new site"
      }
    },
    {
      "type": "verifyTitle",
      "title": "Welcome to Skeletor | Skeletor"
    }
  ]
}
